module RoboCompfaceDetectorDemo{
	struct faceBB{
		int x0;
		int y0;
		int x1;
		int y1;
	};
	
	sequence <byte> rgbSeq;
	
	struct imageRGB {
		rgbSeq rgbPixel;
		int width;
		int height;
	};
	
	sequence <faceBB> facesBB;
	struct ListOfFaces{
		facesBB faceList;
	};
	
	interface faceDetectorDemo{
		[query] void faceDetection(imageRGB image, out ListOfFaces faces);
	};
};
