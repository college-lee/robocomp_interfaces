module RoboCompFullPoseEstimation
{
    exception HardwareFailedException { string what; };

    struct FullPose
    {
        float x;
        float y;
	float z;
        float rx;
	float ry;
	float rz;
    };

    interface FullPoseEstimation
    {
        FullPose getFullPose() throws HardwareFailedException;
    };
};
